.sidebar-component
  .title-override
    text-transform none
  
  .sidebar-button-component
    background-color #fff
    border none
    border-radius 0px
    color #888888
    font-family "Open Sans", "Helvetica Neue", Helvetica, Arial, sans-serif
    font-size 1.8rem
    font-weight 300
    margin 0
    padding 10px 20px
    text-align left
    width 100%

    em  
      margin-right 5px

    &:hover
      background-color #F9F9F9
      color #35cebe

    